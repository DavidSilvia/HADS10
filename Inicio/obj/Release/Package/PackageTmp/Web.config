<configuration>
    <connectionStrings>
        <add name="HADS10_TareasConnectionString" connectionString="$(ReplacableToken_HADS10_TareasConnectionString-Web.config Connection String_0)"
            providerName="System.Data.SqlClient" />
        <add name="HADS10_TareasConnectionString2" connectionString="$(ReplacableToken_HADS10_TareasConnectionString2-Web.config Connection String_0)"
            providerName="System.Data.SqlClient" />
    </connectionStrings>
    <system.web>
		<authentication mode="Forms">
			<forms loginUrl="Inicio.aspx" protection="All" name=".ASPXFORMSAUTH"/>
		</authentication>
		<authorization>
			<deny users="?"/>
		</authorization>
      <compilation strict="false" explicit="true" targetFramework="4.0" />
	<customErrors mode="Off"/>
    </system.web>

	<location path="Profesor">
		<system.web>

			<authorization>
				<deny users="Alumno"/>
			</authorization>
		</system.web>
	</location>

	<location path="Profesor/ExportarTareas.aspx">
	<system.web>
		<authorization>
			<allow users="vadillo@ehu.es"/>
			<deny users="*"/>
		</authorization>
	</system.web>
	</location>

</configuration>
